<%= form_for( [:admin, @object], :remote => true) do |f| %>
  <div class="container">
  	<div class='span-8'>
	  <%= f.field_container :tel_type do %>
	  	<%= f.label :tel_type %><br />
	  	<%= f.text_field :tel_type %>
	  <% end %>
	  <%= f.field_container :tel do %>
	  	<%= f.label :tel %><br />
	  	<%= f.text_field :tel %>
	  <% end %>
	  </div>
	  <div class='span-8 last'>
	 	</div>
	</div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>