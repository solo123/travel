<%= form_for( [:admin, @object], :remote => true) do |f| %>
  <div class="container">
  	<div class='span-8'>
	  <%= f.field_container :type_name do %>
	  	<%= f.label :type_name %><br />
	  	<%= f.text_field :type_name %>
	  <% end %>
		<%= render :partial => 'admin/shared/status', :locals => {:f => f} %> 
	  </div>
	  <div class='span-8 last'>
	 	</div>
	</div>
      
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
